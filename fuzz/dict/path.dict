# libfuzzer dictionary for path fuzzing

# Path separators
"/"
"\\"
"./"
"../"

# Common path components
"src"
"lib"
"crates"
"packages"
"tests"
"docs"
"target"
"node_modules"
"vendor"

# Additional common directories
"bin"
"pkg"
"dist"
"out"
"build"
"scripts"
"tools"
"assets"
"resources"
"config"
"data"
"tmp"
"temp"
"cache"
"logs"

# VCS and CI directories
".git"
".github"
".gitlab"
".svn"
".hg"
".circleci"

# Rust-specific paths
"benches"
"examples"
"build.rs"
"Cargo.toml"
"Cargo.lock"
"rust-toolchain"
"rust-toolchain.toml"
".cargo"
"clippy.toml"
"rustfmt.toml"

# Module root patterns that tokmd uses
"crates/"
"crates/*/src"
"packages/"
"packages/*/src"
"libs/"
"libs/*/src"
"src/"
"lib/"
"src/lib.rs"
"src/main.rs"

