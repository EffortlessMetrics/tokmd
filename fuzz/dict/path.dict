# Special paths
"."
".."
"~"
""

# Edge cases
"a/b/c/d/e"
"a/./b/../c"
"//double/slash"
"path with spaces"
"path-with-dashes"
"path_with_underscores"
"PascalCasePath"
"camelCasePath"

# Deeply nested paths
"a/b/c/d/e/f/g/h/i/j"
"crates/tokmd/src/lib.rs"
"packages/core/src/index.ts"

# Hidden files/directories
".hidden"
".config"
".cache"
".local"
"._metadata"
